    "chai-as-promised": "^7.1.1",
    "copy-webpack-plugin": "6.4.1",
    "copyfiles": "^2.4.1",
    "cuelang-js": "1.3.0",
    "husky": "^4.2.5",
    "karma": "^5.0.9",
